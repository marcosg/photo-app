<div class="container" >
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Name:</strong>
    <%= @image.name %>
  </p>

  <p>
    <strong>Picture:</strong>
    <%= image_tag(@image.picture.url, height:"300px") if @image.picture? %>
  </p>


  <%= link_to 'Edit', edit_image_path(@image) %> |
  <%= link_to 'Back', images_path %>
</div>